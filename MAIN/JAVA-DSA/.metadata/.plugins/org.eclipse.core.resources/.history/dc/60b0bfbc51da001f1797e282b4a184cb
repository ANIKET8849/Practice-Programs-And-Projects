package QuickSort;

public class QuickSort {
	
	

	void printarr(int arr[]) {
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i]+" ");
		}
	}
	public static void main(String[] args) {
		int arr[]= {3,7,44,3,8,5,4,55};
		QuickSort qSort =new QuickSort();
		qSort.printarr(arr);
	}

}
